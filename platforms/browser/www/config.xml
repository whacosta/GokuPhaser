<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.gokuphaser" version="2.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>GokuPhaser</name>
    <description>
        Juego de esquivar objetos usando el acelerometro.
    </description>
    <author email="whacosta@fiec.espol.ecu.ec" href="https://github.com/whacosta/GokuPhaser">
        Wilmer Acosta
    </author>
    <content src="index.html" />
    <allow-navigation href="http://*/*" />
    <allow-intent href="https://*/*" />
    <access origin="*" />
    <preference name="phonegap-version" value="cli-7.0.1" />
    <preference name="Orientation" value="portrait" />
</widget>
